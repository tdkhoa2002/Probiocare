{
  "name": "cryperrcms/blockchain",
  "description": "The Cryperr CMS for Blockchain application.",
  "keywords": [
    "cms",
    "asgardcms",
    "Cryperr CMS",
    "Blockchain",
    "multilingual",
    "laravel",
    "laravel8"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0.2",
    "astrotomic/laravel-translatable": "^11.9",
    "cartalyst/sentinel": "^6.0",
    "doctrine/dbal": "^2.11",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.3.0",
    "guzzlehttp/psr7": "^1.8.2",
    "hashids/hashids": "^5.0",
    "intervention/image": "^2.6",
    "laracasts/presenter": "^0.2.4",
    "laravel/framework": "^9.0",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.0",
    "laravelcollective/html": "^6.0",
    "league/csv": "^9.0",
    "maatwebsite/laravel-sidebar": "^2.4",
    "mcamara/laravel-localization": "^1.6",
    "nwidart/laravel-modules": "^8.0",
    "pusher/pusher-php-server": "^7.0",
    "ramsey/uuid": "^4.0",
    "tatumio/tatum-php": "^2.0",
    "tightenco/ziggy": "^0.9.4",
    "typicms/nestablecollection": "^1.1",
    "venturecraft/revisionable": "^1.35",
    "web3p/web3.php": "dev-master",
    "yajra/laravel-datatables-oracle": "~9.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "mockery/mockery": "^1.3.1",
    "orchestra/testbench": "^7.14.0",
    "phpunit/phpunit": "~9.5.10",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/",
      "Themes\\": "Themes/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/Helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    },
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php artisan clear-compiled"
    ],
    "pre-update-cmd": [],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
